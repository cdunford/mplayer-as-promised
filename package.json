{
  "name": "mplayer-as-promised",
  "version": "0.0.1",
  "description": "Simple mplayer (http://www.mplayerhq.hu/) wrapper for use in node written in Typescript and presenting a promise-heavy API",
  "main": "dist/lib/index.js",
  "types": "dist/typings/lib/index.d.ts",
  "scripts": {
    "prebuild": "IF EXIST dist rmdir /S /Q dist",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node dist/example.js",
    "pretest": "npm run build",
    "test": "mocha \"dist/test/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdunford/mplayer-as-promised.git"
  },
  "keywords": [
    "mplayer"
  ],
  "author": "Craig Dunford",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cdunford/mplayer-as-promised/issues"
  },
  "homepage": "https://github.com/cdunford/mplayer-as-promised#readme",
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.8",
    "@types/sinon": "^1.16.36",
    "@types/sinon-chai": "^2.7.27",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0",
    "typescript": "^2.2.1"
  }
}